{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      false,
      "element",
      "app",
      "kebab-case"
    ],
    "max-line-length": false,
    "jsdoc-format": false
  }
}
